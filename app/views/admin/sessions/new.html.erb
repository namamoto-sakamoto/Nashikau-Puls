<h3>管理者 Log in</h3>
<div class="row">
  <div class="col-8 offset-1">
    <%= form_with model: @admin, url: session_path(resource_name), local: true do |f| %>
      <div class="my-4">
        <%= f.label :"メールアドレス" %><br>
        <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
      </div>
    
      <div class="my-4">
        <%= f.label :"パスワード" %><br>
        <%= f.password_field :password, autocomplete: "current_password" %>
      </div>
      
      <div>
        <%= f.button :type => "submit", class: 'btn btn-success mt-3' do %>
          <div>　Log innn!+　</div>
        <% end %>  
      </div>
    <% end %>
  </div>  
</div>    